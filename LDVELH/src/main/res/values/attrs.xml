<resources>

    <declare-styleable name="AnimatedNumber">
        <attr name="nbDigits" format="integer" />
        <attr name="animOrientation" format="string" />
    </declare-styleable>
    <declare-styleable name="CustomNumberPicker">
        <attr name="vertical" format="boolean" />
        <attr name="scaleFactor" format="float" />
        <attr name="allowLongPress" format="boolean" />
    </declare-styleable>
    <declare-styleable name="CustomListView">
        <attr name="listPropertyName" format="string" />
        <attr name="rowLayout" format="string" />
        <attr name="newItemVisible" format="boolean" />
        <attr name="newConsumableItemVisible" format="boolean" />
        <attr name="newItemCaption" format="string" />
        <attr name="newConsumableItemCaption" format="string" />
        <attr name="storeClass" format="string" />
    </declare-styleable>
    <declare-styleable name="ShopPickerItem">
        <attr name="itemName" format="string" />
        <attr name="price" format="integer" />
    </declare-styleable>
    <declare-styleable name="DF04CrewMember">
        <attr name="crewMemberProperty" format="string" />
    </declare-styleable>
    <declare-styleable name="CCAttribute">
        <attr name="attributeName" format="string" />
        <attr name="textStyle" format="reference" />
        <attr name="withPicker" format="boolean" />
        <attr name="propertyName" format="string" />
        </declare-styleable>
    <declare-styleable name="SOSpellButton">
        <attr name="name" format="string" />
        <attr name="cost" format="integer" />
        <attr name="ingredient" format="string" />
    </declare-styleable>

</resources>